<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.kenyaemrIL">


	<class name="KenyaEmrOutbox"
		table="${project.parent.artifactId}_outbox">
		<id name="id" type="int" column="id" unsaved-value="0">
			<generator class="native" />
		</id>
		<discriminator column="id" insert="false" />
		<property name="uuid" type="java.lang.String" column="uuid"
			length="38" unique="true" />

        <property name="status" column="status" />
        <many-to-one name="ilMessageType" class="org.openmrs.module.kenyaemrIL.ILMessageType"
                     column="messagetype_id" cascade="none"  />
	</class>

</hibernate-mapping>
