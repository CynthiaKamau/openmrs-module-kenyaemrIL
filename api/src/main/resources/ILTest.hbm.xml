<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openmrs.module.kenyaemrIL.il">

    <class name="ILTest" table="il_test">

        <id name="iltestid" type="int" column="il_test_id" unsaved-value="0">
            <generator class="native">
                <param name="sequence">il_test_il_test_id_seq</param>
            </generator>
        </id>

        <discriminator column="il_test_id" insert="false"/>

        <property name="name" type="java.lang.String"
                  column="name" not-null="true" length="255"/>

        <property name="description" type="java.lang.String"
                  column="description" not-null="true" length="2048"/>

        <many-to-one name="creator" class="org.openmrs.User" not-null="true">
            <column name="creator"/>
        </many-to-one>

        <property name="dateCreated" type="java.util.Date"
                  column="date_created" not-null="true" length="19"/>

        <property name="uuid" type="java.lang.String"
                  column="uuid" length="38" unique="true"/>

        <property name="retired" type="boolean" column="retired"
                  length="1" not-null="true" />

        <property name="messageType" type="int" column="message_type"
                  length="1" not-null="true" />
    </class>
</hibernate-mapping>
